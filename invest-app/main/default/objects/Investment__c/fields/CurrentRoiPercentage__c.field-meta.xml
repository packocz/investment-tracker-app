<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CurrentRoiPercentage__c</fullName>
    <description>Investment&apos;s potential gain</description>
    <externalId>false</externalId>
    <formula>IF(InvestedAmount__c == 0, 0, ((CurrentBalance__c + ABS(EarnedAmount__c)) / InvestedAmount__c )) - 1</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Unrealised ROI</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
