<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ActualRoiPercentage__c</fullName>
    <description>Actually earned, cannot be lost anymore</description>
    <externalId>false</externalId>
    <formula>IF(InvestedAmount__c == 0, 0, (ABS(EarnedAmount__c) /  InvestedAmount__c)) - 1</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Realised ROI</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
